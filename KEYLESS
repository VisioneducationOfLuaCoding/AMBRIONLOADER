-- Load Orion Library
local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/darkkcontrol/Roblox-Orion-UI-Libary-OP-UI-LIBARY-/refs/heads/main/source"))()

-- Create Window
local Window = OrionLib:MakeWindow({
    Name = "Ambrion Loader | ALL KEYLESS!",
    HidePremium = false,
    SaveConfig = false,
    IntroText = "Loading Ambrion..."
})

-- Create Tab
local Tab = Window:MakeTab({
    Name = "Loaders",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Arsenal Loader Button
Tab:AddButton({
    Name = "Arsenal",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/VisioneducationOfLuaCoding/Arsenal/refs/heads/main/Ambrion"))()
    end
})

-- Blade Ball Loader Button
Tab:AddButton({
    Name = "Blade Ball",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/VisioneducationOfLuaCoding/BladeBall/refs/heads/main/ScriptLoader", true))()
    end
})

-- Grow a Garden Loader Button
Tab:AddButton({
    Name = "Grow a Garden",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/VisioneducationOfLuaCoding/growagarden/refs/heads/main/Ambrion", true))()
    end
})

-- Steal a Brainrot Loader Button
Tab:AddButton({
    Name = "Steal a Brainrot",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/VisioneducationOfLuaCoding/StealABrainrot/refs/heads/main/Ambrion"))()
    end
})

-- Rivals V3 Loader Button
Tab:AddButton({
    Name = "Rivals V3",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/VisioneducationOfLuaCoding/Ambrion/refs/heads/main/RivalsVersion3", true))()
    end
})

-- Init
OrionLib:Init()
